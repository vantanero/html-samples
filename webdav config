<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <verbs>
                    <add verb="PUT" allowed="true"/>
                    <add verb="DELETE" allowed="true"/>
                </verbs>
            </requestFiltering>
        </security>
        <webDav>
            <authoring enabled="true" requireSsl="false">
                <authoringRules>
                    <add users="*" roles="" verbs="PUT, DELETE, GET, POST, HEAD, OPTIONS, PROPFIND, MOVE, COPY, MKCOL" path="*" />
                </authoringRules>
            </authoring>
        </webDav>
    </system.webServer>
</configuration>

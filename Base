<?php
session_start();
if (!isset($_SESSION["authenticated"]) || $_SESSION["authenticated"] !== true) {
    header("Location: index.html"); // Redirect back to login page if not authenticated
    exit();
}
?>
<!DOCTYPE html>
<html>
<head>
    <title>Success</title>
</head>
<body>
    <h1>Login Successful!</h1>
    <p>Welcome, Admin!</p>
    <a href="logout.php">Logout</a>
</body>
</html>
